{
  "rules": {
    "events": {
      "$uid": {
        ".read": "true",
        ".write": "!data.exists()",
        "imageId": {},
        "location": {},
        "owner": {},
        "description": {}
      }
    },
    "images": {
      "$uid": {
        ".read": "true",
        ".write": "auth !== null",
        "base64": {
          ".validate": "newData.isString()"
        },
        "blur": {
          ".validate": "newData.isString()"
        },
        "path": {
          ".validate": "newData.isString()"
        },
        "thumbnail": {
          ".validate": "newData.isString()"
        }
      }
    },
    "version": {
      ".read": "true",
      ".write": "auth !== null",
      "version": {
        ".validate": "newData.isString()"
      },
      "timestamp": {
        ".validate": "newData.isString()"
      }
    },
    "stats": {
      ".read": "true",
      ".write": "auth !== null"
    }
  }
}
